<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.airplane.mapper.InsuranceMapper">

	<select id="insuranceAll" resultType="com.airplane.insurance.Insurance">
    	SELECT * FROM insurance WHERE id != 0
	</select>
	
	<select id="getInsurance" resultType="com.airplane.insurance.Insurance">
    	SELECT * FROM insurance WHERE id = #{id}
	</select>
</mapper>
  